generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Website {
  id          Int      @id @default(autoincrement())
  url         String   @unique
  brandName   String?
  description String?
  enhancedDescription String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
